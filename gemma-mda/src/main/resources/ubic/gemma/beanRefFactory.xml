<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE beans
    PUBLIC "-//SPRING//DTD BEAN//EN"
           "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

	<bean id="beanRefFactory" class="org.springframework.context.support.ClassPathXmlApplicationContext">
		<constructor-arg>
			<list>
				<value>classpath*:/ubic/gemma/applicationContext-hibernate.xml</value>
				<value>classpath*:/ubic/gemma/dataSource.xml</value>
				<value>classpath*:/ubic/gemma/applicationContext-security.xml</value>
				<value>classpath*:/ubic/gemma/applicationContext-serviceBeans.xml</value>
				<value>classpath*:/ubic/gemma/applicationContext-persisterBeans.xml</value>
				<value>classpath*:/ubic/gemma/compass.xml</value>
			</list>
		</constructor-arg>
	</bean>
	<!-- beanRefFactoryEjbXml merge-point -->

</beans>