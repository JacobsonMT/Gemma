<?xml version="1.0" encoding="ISO-8859-1" ?>
<mappings name="CustomMySQL">
	<extends>MySQL</extends>
	<mapping>
		<from>datatype::URL</from>
		<to>VARCHAR(512)</to>
	</mapping>
	<mapping>
		<from>datatype::byte[]</from>
		<to>TINYBLOB</to>
	</mapping>
	<mapping>
		<from>datatype::String</from>
		<to>VARCHAR(255) character set latin1 collate latin1_swedish_ci</to>
	</mapping>
	<mapping>
		<from>datatype::Clob</from>
		<to>text character set latin1 collate latin1_swedish_ci</to>
	</mapping>
</mappings>
