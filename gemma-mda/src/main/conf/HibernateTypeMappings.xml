<?xml version="1.0" encoding="ISO-8859-1" ?>
<mappings name="CustomHibernate">
	<extends>Hibernate</extends>
	<mapping>
		<from>datatype::Blob</from>
		<to>ubic.gemma.persistence.hibernate.usertypes.HibernateByteBlobType</to>
	</mapping>
	<mapping>
		<from>datatype::Clob</from>
		<to>ubic.gemma.persistence.hibernate.usertypes.HibernateStringClobType</to>
	</mapping>
	<mapping>
		<from>datatype::URL</from>
		<to>ubic.gemma.persistence.hibernate.usertypes.HibernateURLType</to>
	</mapping>
</mappings>

