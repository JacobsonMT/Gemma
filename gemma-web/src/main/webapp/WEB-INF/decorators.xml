<decorators defaultdir="/decorators">
    <excludes>
        <pattern>/resources/*</pattern>
    </excludes> 
    <decorator name="default" page="default.jsp">
        <pattern>/*</pattern>
    </decorator>
    <decorator name="homePage" page="home.jsp">
        <pattern>/mainMenu*</pattern>
    </decorator>
</decorators>
