<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="none">
   <class name="ubic.gemma.model.expression.biomaterial.BioMaterialImpl" table="BIO_MATERIAL" dynamic-insert="false" dynamic-update="false">
      <cache usage="read-write" />
      <id name="id" type="java.lang.Long" unsaved-value="null">
         <column name="ID" sql-type="BIGINT"/>
         <generator class="native">
         </generator>
      </id>
      <many-to-one name="auditTrail" class="ubic.gemma.model.common.auditAndSecurity.AuditTrailImpl"  cascade="all" lazy="proxy" fetch="select">
         <column name="AUDIT_TRAIL_FK" not-null="false" sql-type="BIGINT" unique="true"/>
      </many-to-one>
      <property name="name" type="java.lang.String">
         <column name="NAME" not-null="false" unique="false" sql-type="VARCHAR(255) character set latin1 collate latin1_swedish_ci"/>
      </property>
      <property name="description" type="org.springframework.orm.hibernate3.support.ClobStringType">
         <column name="DESCRIPTION" not-null="false" unique="false" sql-type="text character set latin1 collate latin1_swedish_ci"/>
      </property>
      <many-to-one name="sourceTaxon" class="ubic.gemma.model.genome.TaxonImpl"  lazy="proxy" fetch="select">
         <column name="SOURCE_TAXON_FK" not-null="true" sql-type="BIGINT"/>
      </many-to-one>
      <set name="factorValues" table="BIO_MATERIAL_FACTOR_VALUES" lazy="false" fetch="select" inverse="false">
         <key foreign-key="FACTOR_VALUE_BIO_MATERIALS_FKC">
            <column name="BIO_MATERIALS_FK" sql-type="BIGINT"/>
         </key>
         <many-to-many class="ubic.gemma.model.expression.experiment.FactorValueImpl" >
            <column name="FACTOR_VALUES_FK" sql-type="BIGINT"/>
         </many-to-many>
      </set>
      <set name="bioAssaysUsedIn" lazy="false" fetch="select" inverse="true">
         <cache usage="read-write" />
         <key foreign-key="BIO_ASSAY_SAMPLE_USED_FKC">
            <column name="SAMPLE_USED_FK" sql-type="BIGINT"/>
         </key>
         <one-to-many class="ubic.gemma.model.expression.bioAssay.BioAssayImpl"/>
      </set>
      <set name="treatments"  lazy="true" fetch="select" inverse="false" cascade="all">
         <cache usage="read-write" />
         <key foreign-key="TREATMENT_BIO_MATERIALS_FKC">
            <column name="BIO_MATERIALS_FK" sql-type="BIGINT"/>
         </key>
         <one-to-many class="ubic.gemma.model.expression.biomaterial.TreatmentImpl"/>
      </set>
      <set name="characteristics" lazy="false" fetch="select" inverse="false" cascade="all">
         <cache usage="read-write" />
         <key foreign-key="CHARACTERISTIC_BIO_MATERIAL_FKC">
            <column name="BIO_MATERIAL_FK" sql-type="BIGINT"/>
         </key>
         <one-to-many class="ubic.gemma.model.common.description.CharacteristicImpl"/>
      </set>
      <many-to-one name="externalAccession" class="ubic.gemma.model.common.description.DatabaseEntryImpl"  cascade="all" lazy="false" fetch="join">
         <column name="EXTERNAL_ACCESSION_FK" not-null="false" sql-type="BIGINT" unique="true"/>
      </many-to-one>
   </class>
</hibernate-mapping>