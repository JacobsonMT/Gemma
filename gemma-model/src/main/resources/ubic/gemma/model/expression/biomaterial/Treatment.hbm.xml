<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="none">
   <class name="ubic.gemma.model.expression.biomaterial.TreatmentImpl" table="TREATMENT" dynamic-insert="false" dynamic-update="false">
      <cache usage="read-write" />
      <id name="id" type="java.lang.Long" unsaved-value="null">
         <column name="ID" sql-type="BIGINT"/>
         <generator class="native">
         </generator>
      </id>
      <many-to-one name="auditTrail" class="ubic.gemma.model.common.auditAndSecurity.AuditTrailImpl"  cascade="all" lazy="proxy" fetch="select">
         <column name="AUDIT_TRAIL_FK" not-null="false" sql-type="BIGINT" unique="true"/>
      </many-to-one>
      <many-to-one name="status" class="ubic.gemma.model.common.auditAndSecurity.StatusImpl"  cascade="all" lazy="proxy" fetch="select">
         <column name="STATUS_FK" not-null="false" sql-type="BIGINT" unique="true"/>
      </many-to-one>
      <property name="name" type="java.lang.String">
         <column name="NAME" not-null="false" unique="false" sql-type="VARCHAR(255) character set latin1 collate latin1_swedish_ci"/>
      </property>
      <property name="description" type="org.springframework.orm.hibernate3.support.ClobStringType">
         <column name="DESCRIPTION" not-null="false" unique="false" sql-type="text character set latin1 collate latin1_swedish_ci"/>
      </property>
      <property name="orderApplied" type="java.lang.Integer">
         <column name="ORDER_APPLIED" not-null="true" unique="false" sql-type="INTEGER"/>
      </property>
      <many-to-one name="actionMeasurement" class="ubic.gemma.model.common.measurement.MeasurementImpl"  cascade="none" lazy="proxy" fetch="select">
         <column name="ACTION_MEASUREMENT_FK" not-null="false" sql-type="BIGINT" unique="true"/>
      </many-to-one>
      <set name="protocolApplications"  lazy="false" fetch="select" inverse="false" cascade="all">
         <cache usage="read-write" />
         <key foreign-key="PROTOCOL_APPLICATION_TREATMENT_FKC">
            <column name="TREATMENT_FK" sql-type="BIGINT"/>
         </key>
         <one-to-many class="ubic.gemma.model.common.protocol.ProtocolApplicationImpl"/>
      </set>
      <many-to-one name="action" class="ubic.gemma.model.common.description.CharacteristicImpl"  cascade="all" lazy="false" fetch="select">
         <column name="ACTION_FK" not-null="false" sql-type="BIGINT" unique="true"/>
      </many-to-one>
   </class>
</hibernate-mapping>