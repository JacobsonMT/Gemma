<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="none">
   <class name="ubic.gemma.model.common.auditAndSecurity.AuditTrailImpl" table="AUDIT_TRAIL" dynamic-insert="false" dynamic-update="false">
      <cache usage="read-write" />
      <id name="id" type="java.lang.Long" unsaved-value="null">
         <column name="ID" sql-type="BIGINT"/>
         <generator class="native">
         </generator>
      </id>
      <bag name="events" order-by="date" lazy="false" fetch="select" inverse="false" cascade="all">
         <cache usage="read-write" />
         <key foreign-key="AUDIT_EVENT_AUDIT_TRAIL_FKC">
            <column name="AUDIT_TRAIL_FK" sql-type="BIGINT"/>
         </key>
         <one-to-many class="ubic.gemma.model.common.auditAndSecurity.AuditEventImpl"/>
      </bag>
   </class>
</hibernate-mapping>