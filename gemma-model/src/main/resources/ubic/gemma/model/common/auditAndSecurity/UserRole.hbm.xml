<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="none">
   <class name="ubic.gemma.model.common.auditAndSecurity.UserRoleImpl" table="USER_ROLE" dynamic-insert="false" dynamic-update="false">
      <cache usage="read-write" />
      <id name="id" type="java.lang.Long" unsaved-value="null">
         <column name="ID" sql-type="BIGINT"/>
         <generator class="native">
         </generator>
      </id>
      <property name="name" type="java.lang.String">
         <column name="NAME" not-null="false" unique="false" sql-type="VARCHAR(255) character set latin1 collate latin1_swedish_ci"/>
      </property>
      <property name="description" type="ubic.gemma.persistence.hibernate.usertypes.HibernateStringClobType">
         <column name="DESCRIPTION" not-null="false" unique="false" sql-type="text character set latin1 collate latin1_swedish_ci"/>
      </property>
      <property name="userName" type="java.lang.String">
         <column name="USER_NAME" not-null="true" unique="false" sql-type="VARCHAR(255) character set latin1 collate latin1_swedish_ci"/>
      </property>
   </class>
</hibernate-mapping>