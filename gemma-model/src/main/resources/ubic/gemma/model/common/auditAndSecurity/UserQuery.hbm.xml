<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="none">
   <class name="ubic.gemma.model.common.auditAndSecurity.UserQueryImpl" table="USER_QUERY" dynamic-insert="false" dynamic-update="false">
      <cache usage="read-write" />
      <id name="id" type="java.lang.Long" unsaved-value="null">
         <column name="ID" sql-type="BIGINT"/>
         <generator class="native">
         </generator>
      </id>
      <property name="url" type="java.lang.String">
         <column name="URL" not-null="true" unique="false" sql-type="VARCHAR(255) character set latin1 collate latin1_swedish_ci"/>
      </property>
      <property name="name" type="java.lang.String">
         <column name="NAME" not-null="false" unique="false" sql-type="VARCHAR(255) character set latin1 collate latin1_swedish_ci"/>
      </property>
      <property name="lastUsed" type="java.util.Date">
         <column name="LAST_USED" not-null="true" unique="false" sql-type="DATETIME"/>
      </property>
      <property name="autoRunFrequencyHours" type="java.lang.Integer">
         <column name="AUTO_RUN_FREQUENCY_HOURS" not-null="false" unique="false" sql-type="INTEGER"/>
      </property>
      <many-to-one name="searchSettings" class="ubic.gemma.model.common.search.SearchSettingsImpl"  cascade="all" lazy="false" fetch="select">
         <column name="SEARCH_SETTINGS_FK" not-null="false" sql-type="BIGINT" unique="true"/>
      </many-to-one>
   </class>
</hibernate-mapping>