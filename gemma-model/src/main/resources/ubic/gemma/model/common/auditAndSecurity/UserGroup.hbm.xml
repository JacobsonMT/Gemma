<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="none">
   <class name="ubic.gemma.model.common.auditAndSecurity.UserGroupImpl" table="USER_GROUP" dynamic-insert="false" dynamic-update="false">
      <cache usage="read-write" />
      <id name="id" type="java.lang.Long" unsaved-value="null">
         <column name="ID" sql-type="BIGINT"/>
         <generator class="native">
         </generator>
      </id>
      <many-to-one name="auditTrail" class="ubic.gemma.model.common.auditAndSecurity.AuditTrailImpl"  cascade="all" lazy="proxy" fetch="select">
         <column name="AUDIT_TRAIL_FK" not-null="false" sql-type="BIGINT" unique="true"/>
      </many-to-one>
      <many-to-one name="status" class="ubic.gemma.model.common.auditAndSecurity.StatusImpl"  cascade="all" lazy="proxy" fetch="select">
         <column name="STATUS_FK" not-null="false" sql-type="BIGINT" unique="true"/>
      </many-to-one>
      <property name="name" type="java.lang.String">
         <column name="NAME" not-null="false" unique="false" sql-type="VARCHAR(255) character set latin1 collate latin1_swedish_ci"/>
      </property>
      <property name="description" type="ubic.gemma.persistence.hibernate.usertypes.HibernateStringClobType">
         <column name="DESCRIPTION" not-null="false" unique="false" sql-type="text character set latin1 collate latin1_swedish_ci"/>
      </property>
      <set name="groupMembers" table="GROUP_MEMBERS" lazy="false" fetch="select" inverse="false">
         <key foreign-key="USER_USER_GROUPS_FKC">
            <column name="USER_GROUPS_FK" sql-type="BIGINT"/>
         </key>
         <many-to-many class="ubic.gemma.model.common.auditAndSecurity.UserImpl" >
            <column name="GROUP_MEMBERS_FK" sql-type="BIGINT"/>
         </many-to-many>
      </set>
      <set name="authorities" lazy="false" fetch="select" inverse="false" cascade="all">
         <cache usage="read-write" />
         <key foreign-key="GROUP_AUTHORITY_GROUP_FKC">
            <column name="GROUP_FK" sql-type="BIGINT"/>
         </key>
         <one-to-many class="ubic.gemma.model.common.auditAndSecurity.GroupAuthorityImpl"/>
      </set>
   </class>
</hibernate-mapping>