<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="none">
   <class name="ubic.gemma.model.association.LiteratureAssociationImpl" table="LITERATURE_ASSOCIATION" dynamic-insert="false" dynamic-update="false">
      <cache usage="read-write" />
      <id name="id" type="java.lang.Long" unsaved-value="null">
         <column name="ID" sql-type="BIGINT"/>
         <generator class="native">
         </generator>
      </id>
      <many-to-one name="secondGene" class="ubic.gemma.model.genome.GeneImpl"  lazy="proxy" fetch="select">
         <column name="SECOND_GENE_FK" not-null="true" sql-type="BIGINT"/>
      </many-to-one>
      <many-to-one name="firstGene" class="ubic.gemma.model.genome.GeneImpl"  lazy="proxy" fetch="select">
         <column name="FIRST_GENE_FK" not-null="true" sql-type="BIGINT"/>
      </many-to-one>
      <many-to-one name="sourceAnalysis" class="ubic.gemma.model.analysis.Analysis"  lazy="proxy" fetch="select">
         <column name="SOURCE_ANALYSIS_FK" not-null="false" sql-type="BIGINT"/>
      </many-to-one>
      <property name="numberOfMentions" type="java.lang.Integer">
         <column name="NUMBER_OF_MENTIONS" not-null="false" unique="false" sql-type="INTEGER"/>
      </property>
      <property name="action" type="java.lang.String">
         <column name="ACTION" not-null="false" unique="false" sql-type="VARCHAR(255) character set latin1 collate latin1_swedish_ci"/>
      </property>
   </class>
</hibernate-mapping>