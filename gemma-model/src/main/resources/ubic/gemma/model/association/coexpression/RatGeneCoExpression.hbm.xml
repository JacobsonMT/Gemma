<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="none">
	<class name="ubic.gemma.model.association.coexpression.RatGeneCoExpressionImpl" table="RAT_GENE_COEXPRESSION"
		dynamic-insert="true" dynamic-update="true" mutable="true">
		<id access="field" name="id" type="java.lang.Long" unsaved-value="null">
			<column name="ID" sql-type="BIGINT" />
			<generator class="org.hibernate.id.MultipleHiLoPerTableGenerator" />
		</id>
		<property access="field" name="positiveCorrelation" type="java.lang.Boolean">
			<column name="POSITIVE" not-null="false" unique="false" sql-type="TINYINT" />
		</property>
		<property access="field" name="numDataSetsSupporting" type="java.lang.Integer">
			<column name="SUPPORT" not-null="false" unique="false" sql-type="INTEGER" />
		</property>
		<many-to-one access="field" name="secondGene" class="ubic.gemma.model.genome.GeneImpl" lazy="proxy" fetch="select"
			cascade="none">
			<column name="SECOND_GENE_FK" not-null="true" sql-type="BIGINT" />
		</many-to-one>
		<many-to-one access="field" name="firstGene" class="ubic.gemma.model.genome.GeneImpl" lazy="proxy" fetch="select"
			cascade="none">
			<column name="FIRST_GENE_FK" not-null="true" sql-type="BIGINT" />
		</many-to-one>
		<many-to-one access="field" name="supportDetails"
			class="ubic.gemma.model.analysis.expression.coexpression.RatCoexpressionSupportDetailsImpl" lazy="proxy" fetch="join"
			cascade="none">
			<column name="SUPPORT_DETAILS_FK" not-null="false" sql-type="BIGINT" />
		</many-to-one>
	</class>
</hibernate-mapping>
