<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="none">
   <class name="ubic.gemma.model.common.search.SearchSettingsImpl" table="SEARCH_SETTINGS" dynamic-insert="false" dynamic-update="false">
      <cache usage="read-write" />
      <id name="id" type="java.lang.Long" unsaved-value="null">
         <column name="ID" sql-type="BIGINT"/>
         <generator class="native">
         </generator>
      </id>
      <property name="query" type="java.lang.String">
         <column name="QUERY" not-null="false" unique="false" sql-type="VARCHAR(255) character set latin1 collate latin1_swedish_ci"/>
      </property>
      <property name="termUri" type="java.lang.String">
         <column name="TERM_URI" not-null="false" unique="false" sql-type="VARCHAR(255) character set latin1 collate latin1_swedish_ci"/>
      </property>
      <property name="maxResults" type="java.lang.Integer">
         <column name="MAX_RESULTS" not-null="true" unique="false" sql-type="INTEGER"/>
      </property>
      <property name="searchExperiments" type="java.lang.Boolean">
         <column name="SEARCH_EXPERIMENTS" not-null="true" unique="false" sql-type="TINYINT"/>
      </property>
      <property name="searchGenes" type="java.lang.Boolean">
         <column name="SEARCH_GENES" not-null="true" unique="false" sql-type="TINYINT"/>
      </property>
      <property name="searchPlatforms" type="java.lang.Boolean">
         <column name="SEARCH_PLATFORMS" not-null="true" unique="false" sql-type="TINYINT"/>
      </property>
      <property name="searchExperimentSets" type="java.lang.Boolean">
         <column name="SEARCH_EXPERIMENT_SETS" not-null="true" unique="false" sql-type="TINYINT"/>
      </property>
      <property name="searchPhenotypes" type="java.lang.Boolean">
         <column name="SEARCH_PHENOTYPES" not-null="true" unique="false" sql-type="TINYINT"/>
      </property>
      <property name="searchProbes" type="java.lang.Boolean">
         <column name="SEARCH_PROBES" not-null="true" unique="false" sql-type="TINYINT"/>
      </property>
      <property name="searchGeneSets" type="java.lang.Boolean">
         <column name="SEARCH_GENE_SETS" not-null="true" unique="false" sql-type="TINYINT"/>
      </property>
      <property name="searchBioSequences" type="java.lang.Boolean">
         <column name="SEARCH_BIO_SEQUENCES" not-null="true" unique="false" sql-type="TINYINT"/>
      </property>
      <property name="searchBibrefs" type="java.lang.Boolean">
         <column name="SEARCH_BIBREFS" not-null="true" unique="false" sql-type="TINYINT"/>
      </property>
      <property name="useIndices" type="java.lang.Boolean">
         <column name="USE_INDICES" not-null="true" unique="false" sql-type="TINYINT"/>
      </property>
      <property name="useDatabase" type="java.lang.Boolean">
         <column name="USE_DATABASE" not-null="true" unique="false" sql-type="TINYINT"/>
      </property>
      <property name="useCharacteristics" type="java.lang.Boolean">
         <column name="USE_CHARACTERISTICS" not-null="true" unique="false" sql-type="TINYINT"/>
      </property>
      <property name="useGo" type="java.lang.Boolean">
         <column name="USE_GO" not-null="true" unique="false" sql-type="TINYINT"/>
      </property>
      <many-to-one name="taxon" class="ubic.gemma.model.genome.Taxon"  lazy="proxy" fetch="select">
         <column name="TAXON_FK" not-null="false" sql-type="BIGINT"/>
      </many-to-one>
      <many-to-one name="platformConstraint" class="ubic.gemma.model.expression.arrayDesign.ArrayDesign"  lazy="proxy" fetch="select">
         <column name="PLATFORM_CONSTRAINT_FK" not-null="false" sql-type="BIGINT"/>
      </many-to-one>
   </class>
</hibernate-mapping>