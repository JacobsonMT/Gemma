<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="none">
	<class name="ubic.gemma.model.expression.experiment.GeeqImpl" table="GEEQ" dynamic-insert="false" dynamic-update="false">
		<cache usage="read-write" />
		<id name="id" type="java.lang.Long" unsaved-value="null">
			<column name="ID" sql-type="BIGINT" />
			<generator class="native">
			</generator>
		</id>
		<property name="suitability" type="java.lang.Double">
			<column name="SUITABILITY" not-null="false" unique="false" sql-type="DOUBLE" />
		</property>
		<property name="quality" type="java.lang.Double">
			<column name="QUALITY" not-null="false" unique="false" sql-type="DOUBLE" />
		</property>
		<!-- other fields to be added... -->
		<!--Double distExpSize; Double distPlatCount; Double distPlatCoverage; Double distPlatMissingValues; Double distPlatPopularity; 
			Double scoreExpBatchInfo; Double scoreExpPublication; Double scoreExpQT; Double scoreExpSize; Double scorePlatCount; Double 
			scorePlatCoverage; Double scorePlatMissingValues; Double scorePlatPopularity; Double scorePlatTroubled; -->
	</class>
</hibernate-mapping>
                         