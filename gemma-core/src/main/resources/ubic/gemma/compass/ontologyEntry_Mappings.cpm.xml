<compass-core-mapping package="ubic.gemma.model">

<class name="common.description.OntologyEntryImpl" alias="ontologyEntry" root="true">

    <id name="id" />
   
    <property name="description">
      <meta-data>description</meta-data>
      <meta-data>array design description</meta-data>
    </property>
    
    <property name="accession">
      <meta-data>accession</meta-data>
      <meta-data>GO Accession</meta-data>
    </property>
      
         <property name="value">
      <meta-data>value</meta-data>      
    </property>
    
       <property name="category">
      <meta-data>category</meta-data>
      <meta-data>GO category</meta-data>
    </property>
    
 <!-- <component name="associations" ref-alias="children" /> -->
   
</class>    
 

