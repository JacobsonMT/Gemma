<?xml version="1.0"?>
<!DOCTYPE compass-core-mapping PUBLIC
    "-//Compass/Compass Core Mapping DTD 2.0//EN"
    "http://www.compass-project.org/dtd/compass-core-mapping-2.1.dtd">
<!--
	This document is a Compass mapping file, allowing the Gemma objects to be 
	indexed and searched.  All "searchable"  attributes 
	(as well as associations) should be included here.
	
	author: klc / paul
	version: $Id$
-->
<compass-core-mapping package="ubic.gemma.model">
	<class name="genome.biosequence.BioSequenceImpl" alias="biologicalCharacteristic" root="true">
		<id name="id" />
		<property name="name">
			<meta-data>name</meta-data>
		</property>
		<property name="description">
			<meta-data>description</meta-data>
		</property>
		<component name="bioSequence2GeneProduct" ref-alias="bioSequence2GeneProduct" />
	</class>
	<!--  Little woried about this as bioSequence2GeneProduct is an abstract class.  There are 3 concrete subclasses...
		currently using BlatAssociationImpl... if bioSequence2GeneProduct is used bombs when unmarsheling.  -->
	<class name="genome.sequenceAnalysis.BlatAssociationImpl" alias="bioSequence2GeneProduct" root="false">
		<id name="id" />
	</class>
</compass-core-mapping>