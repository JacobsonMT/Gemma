<?xml version="1.0"?>
<!DOCTYPE compass-core-mapping PUBLIC
    "-//Compass/Compass Core Mapping DTD 1.0//EN"
    "http://www.opensymphony.com/compass/dtd/compass-core-mapping.dtd">

<!--
	This document is a Compass mapping file, allowing the Gemma objects to be 
	indexed and searched.  All "searchable" expression experiment attributes 
	(as well as associations) should be included here.
	
	author: klc
	version: $Id$
-->


<compass-core-mapping package="ubic.gemma.model">



	<class name="expression.designElement.CompositeSequenceImpl"
		alias="compositeSequence" root="true">

		<id name="id" />

		<property name="name">
			<meta-data store="no">name</meta-data>
		</property>


		<property name="description">
			<meta-data store="no">description</meta-data>
		</property>

		<component name="biologicalCharacteristic"	ref-alias="biologicalCharacteristic" />			
		<component name="componentReporters" ref-alias="componentReporters" />


	</class>
	<class name="genome.biosequence.BioSequenceImpl"
		alias="biologicalCharacteristic" root="false">

		<id name="id" />

		<property name="name">
			<meta-data store="no">name</meta-data>
		</property>

		<property name="description">
			<meta-data store="no">description</meta-data>
		</property>

		<component name="bioSequence2GeneProduct" ref-alias="bioSequence2GeneProduct" />
		
	</class>
	
	<class name="expression.designElement.ReporterImpl"
		alias="componentReporters" root="false">

		<id name="id" />

		<property name="name">
			<meta-data store="no">name</meta-data>
		</property>

		<property name="description">
			<meta-data store="no">name</meta-data>
		</property>
		
		<property name="strand">
			<meta-data store="no">name</meta-data>
		</property>

	</class>
	
	
	<class name="association.BioSequence2GeneProductImpl"
		alias="bioSequence2GeneProduct" root="false">

		<id name="id" />

		<component name="geneProduct" ref-alias="geneProduct" />

	</class>
	

	<class name="genome.gene.GeneProductImpl"
		alias="geneProduct" root="false">

		<id name="id" />

		<property name="name">
			<meta-data store="no">name</meta-data>
		</property>

		<property name="description">
			<meta-data store="no">description</meta-data>
		</property>


	</class>
	
	</compass-core-mapping>