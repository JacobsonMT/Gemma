	<modelVersion>4.0.0</modelVersion>
	<groupId>gemma</groupId>
	<artifactId>gemma-core</artifactId>
	<name>Gemma Core</name>
	<version>1.6-SNAPSHOT</version>
	<description>Module containing core gemma functionality.</description>
	<build>
		<sourceDirectory>src/main/java</sourceDirectory>
		<scriptSourceDirectory>src/main/scripts</scriptSourceDirectory>
		<testSourceDirectory>src/test/java</testSourceDirectory>
		<outputDirectory>target/classes</outputDirectory>
		<testOutputDirectory>target/test-classes</testOutputDirectory>
		<resources>
			<resource>
				<directory>src/main/resources</directory>
				<includes>
					<include>**/*</include>
				</includes>
			</resource>
			<resource>
				<directory>target/generated-sources/resources</directory>
				<includes>
					<include>**/*</include>
				</includes>
			</resource>
		</resources>
		<testResources>
			<testResource>
				<directory>src/test/resources</directory>
				<includes>
					<include>**/*.properties</include>
					<include>**/*.xml</include>
					<include>**/*.txt</include>
					<include>**/*.fa</include>
					<include>**/*_target</include>
					<include>**/*_probe</include>
					<include>**/*.gz</include>
					<include>**/*.zip</include>
					<include>**/*.probes</include>
					<include>**/*.txt.short</include>
					<include>**/*.sort</include>
					<include>**/*.dat</include>
					<include>**/*.owl</include>
					<include>**/*.fasta</include>
					<include>**/*.nib</include>
					<include>**/*.gff</include>
					<include>**/*.sql</include>
				</includes>
			</testResource>
		</testResources>
		<directory>target</directory>
		<finalName>${artifactId}-${version}</finalName>
		<plugins>
			<plugin>
				<artifactId>maven-antrun-plugin</artifactId>
				<executions>
					<execution>
						<id>Generate version file</id>
						<phase>process-classes</phase>
						<goals>
							<goal>run</goal>
						</goals>
						<configuration>
							<tasks>
								<echo append="false" file="${project.build.directory}/classes/version.properties">
									gemma.version=${project.version} built ${maven.build.timestamp}</echo>
							</tasks>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
	<profiles>
		<profile>
			<id>Slow tests</id>
			<activation>
				<property>
					<name>alltests</name>
				</property>
			</activation>
			<build>
				<plugins>
					<plugin>
						<artifactId>maven-surefire-plugin</artifactId>
						<configuration>
							<excludes>
								<exclude>**/*Abstract*</exclude>
							</excludes>
							<includes>
								<include>**/*Test.java</include>
							</includes>
						</configuration>
					</plugin>
				</plugins>
			</build>
		</profile>
	</profiles>
