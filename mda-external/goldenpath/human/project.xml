<?xml version="1.0" encoding="UTF-8"?>
<project>
    <pomVersion>3</pomVersion>
    <extend>../../mda/project.xml</extend>
    <artifactId>gemma-mda</artifactId>
    <name>gemma MDA</name>
    <groupId>Gemma</groupId>
    <dependencies>
        <dependency>
            <groupId>andromda</groupId>
            <artifactId>andromda-profile</artifactId>
            <version>3.0-RC2-SNAPSHOT</version>
            <type>xml.zip</type>
        </dependency>
        <dependency>
            <groupId>andromda</groupId>
            <artifactId>maven-andromda-plugin</artifactId>
            <version>3.0-RC2-SNAPSHOT</version>
            <type>plugin</type>
            <properties>
                <languageMappingsUri>Java</languageMappingsUri>
                <wrapperMappingsUri>JavaWrapper</wrapperMappingsUri>
                <jdbcMappingsUri>JDBC</jdbcMappingsUri>
                <maxSqlNameLength>60</maxSqlNameLength>
                <!--	<foreignKeySuffix>FK</foreignKeySuffix>-->
                <sqlMappingsUri>MySQL</sqlMappingsUri>
            </properties>
        </dependency>
        <dependency>
            <groupId>mysql</groupId>
            <artifactId>mysql</artifactId>
            <jar>mysql-connector-java-3.1.6-bin.jar</jar>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>andromda</groupId>
            <artifactId>andromda-java-cartridge</artifactId>
            <version>3.0-RC2-SNAPSHOT</version>
            <properties>
                <enumerations> 
                    ${maven.andromda.hibernate.generated.dir}</enumerations>
            </properties>
        </dependency>
        <dependency>
            <groupId>andromda</groupId>
            <artifactId>andromda-schema2xmi</artifactId>
            <version>3.0-RC2-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>andromda</groupId>
            <artifactId>andromda-ocl-query-library</artifactId>
            <version>3.0-RC2-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>andromda</groupId>
            <artifactId>andromda-ocl-validation-library</artifactId>
            <version>3.0-RC2-SNAPSHOT</version>
        </dependency>
    </dependencies>
</project>