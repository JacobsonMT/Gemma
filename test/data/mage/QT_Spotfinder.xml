            <SpecializedQuantitationType 
                identifier="TIGR:Spotfinder_QuantitationType:QC" name="QC" 
                isBackground="false">
                <Descriptions_assnlist>
                    <Description 
                        text="Spot total QC score. This is a mean of QC scores in channels A and B.">
                    </Description>
                </Descriptions_assnlist>
                <Scale_assn>
                    <OntologyEntry category="Scale" value="linear_scale">
                    </OntologyEntry>
                </Scale_assn>
                <DataType_assn>
