<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="none">
    <class name="ubic.gemma.model.common.protocol.ProtocolApplicationImpl" table="PROTOCOL_APPLICATION" dynamic-insert="false" dynamic-update="false">
        <cache usage="read-write" />
        <id name="id" type="java.lang.Long" unsaved-value="null">
            <column name="ID" sql-type="BIGINT"/>
            <generator class="native">
            </generator>
        </id>
        <property name="activityDate" type="java.util.Date">
            <column name="ACTIVITY_DATE" not-null="false" unique="false" sql-type="DATETIME"/>
        </property>
        <set name="performers" table="PERFORMERS" lazy="true" fetch="select" inverse="false">
            <key foreign-key="PERSON_PROTOCOL_APPLICATIONS_FKC">
                <column name="PROTOCOL_APPLICATIONS_FK" sql-type="BIGINT"/>
            </key>
            <many-to-many class="ubic.gemma.model.common.auditAndSecurity.PersonImpl" >
                <column name="PERFORMERS_FK" sql-type="BIGINT"/>
            </many-to-many>
        </set>
        <many-to-one name="protocol" class="ubic.gemma.model.common.protocol.ProtocolImpl"  lazy="proxy" fetch="select">
            <column name="PROTOCOL_FK" not-null="true" sql-type="BIGINT"/>
        </many-to-one>
    </class>
</hibernate-mapping>